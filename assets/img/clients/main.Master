          function isNumber(evt) {
            var iKeyCode = (evt.which) ? evt.which : evt.keyCode
            if (iKeyCode != 44 && iKeyCode > 31 && (iKeyCode < 48 || iKeyCode > 58))
              return false;

            return true;
          }
          function isNumberDot(evt) {
            var iKeyCode = (evt.which) ? evt.which : evt.keyCode
            if (iKeyCode != 46 && iKeyCode > 31 && (iKeyCode < 48 || iKeyCode > 58))
              return false;

            return true;
          }
        </script>


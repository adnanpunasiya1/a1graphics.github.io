  <script type="text/javascript">
    function ClickToCall(mobileNumber) {

      // URL for the API endpoint
      var apiUrl = "/../../CRMApplication/AppAPI.asmx/ClickToCall?CustomerMobile=" + mobileNumber + "&UserId=0";

      // Create a new XMLHttpRequest object
      var xhr = new XMLHttpRequest();

      // Configure the request: GET method, URL
      xhr.open("GET", apiUrl, true);

      // Set up the onload function to handle the response
      xhr.onload = function () {
        // Check if the request was successful
        if (xhr.status >= 200 && xhr.status < 300) {
          // Parse the response JSON data
          var responseData = JSON.parse(xhr.responseText);

          if (responseData.response == "1") {
            //alert("Mobile Number Already Entered.");
            DisplayToast("Alert", responseData.response_message, "success");
          }
          else {
            DisplayToast("Alert", responseData.response_message, "error");
          }

          // Process the responseData
          console.log(responseData);
        } else {
          // Request failed, log the error
          console.error("Request failed with status code " + xhr.status);
          DisplayToast("Alert", "Request failed with status code " + xhr.status, "error");
        }
      };

      // Set up the onerror function to handle any errors
      xhr.onerror = function () {
        console.error("Request failed");
        DisplayToast("Alert", "Request failed", "error");
      };

      // Send the request
      xhr.send();

    }

    function ClickToCallCommon() {
      var customer_mobile = $("#txtMobileNumber").val();
      ClickToCall(customer_mobile);
    }
  </script>

